<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{{ title or metadata.title }}</title>
    <meta name="description" content="{{ description or metadata.description }}">
    <link rel="stylesheet" href="{{ '/css/index.css' | url }}">
  </head>
  <body>
    <header>
      <a class="home" href="{{ '/' | url }}">Psicoplast<sup>Â®</sup>
      </a>
      <nav>
        <ul class="nav">
          {%- for entry in collections.pages %}
            <li class="nav-item{% if entry.data.permalink == page.url %} nav-item-active{% endif %}">
              <a href="{{ entry.data.permalink | url }}">{{ entry.data.title }}</a>
            </li>
          {%- endfor %}
        </ul>
      </nav>
    </header>
    <main {% if templateClass %} class="{{ templateClass }}" {% endif %}>
      {{ content | safe }}
    </main>
    <footer></footer>
  </body>
</html></body></html>